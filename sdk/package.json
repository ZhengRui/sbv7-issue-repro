{
  "name": "sdk",
  "version": "0.1.0",
  "description": "SDK",
  "author": "Rui Zheng",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development rollup -c -w --bundleConfigAsCjs",
    "build": "NODE_ENV=production rollup -c --bundleConfigAsCjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint '*.{js,json}' 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix '*.{js,json}' 'src/**/*.{ts,tsx}'",
    "prettier": "prettier -w src .github",
    "prettier:check": "prettier --check src .github",
    "dry-run-release": "semantic-release --dry-run"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/components/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "dist/components"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^18.0.26",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "jest": "^27.4.5",
    "rollup": "^3.9.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "webpack": "5.65.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
  }
}
